using System;

class Program
{
    static void Main(string[] args)
    {
        Hello();
        HelloWhite();
    }

    static void Hello()
    {
        Console.WriteLine("hello");
    }
        static void HelloWhite()
    {
        Console.WriteLine("hello everyone");
    }
    static void greetBlack()
    {
        Console.WriteLine("hello black");
    }
}